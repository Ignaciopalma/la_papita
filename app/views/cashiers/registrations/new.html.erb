<h2>Crear Cajero</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </div>

  <div class="field">        
    <%= f.hidden_field :password, value: "12345678" %>
  </div>

  <div class="field">    
    <%= f.hidden_field :password_confirmation, value: "12345678" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up" %>
  </div>
<% end %>

<%= render "cashiers/shared/links" %>
